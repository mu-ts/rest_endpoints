{
  "name": "@mu-ts/endpoints",
  "version": "3.2.0",
  "description": "Simple REST endpoint and action routing.",
  "main": "./lib/index",
  "types": "./lib/index",
  "author": "matt.filion@gmail.com",
  "homepage": "https://github.com/mu-ts/rest_endpoints#readme",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf coverage lib .nyc_output",
    "clean-all": "rm -rf coverage lib .nyc_output node_modules",
    "test": "nyc mocha -r ts-node/register spec/**/*.spec.ts",
    "coverage": "nyc report --reporter=lcov",
    "coverage-export": "nyc report --reporter=text-lcov > coverage.lcov",
    "lint": "tslint -p .",
    "lint-fix": "tslint --fix -p .",
    "dev-verify": "npm run lint-fix && npm run build"
  },
  "files": [
    "lib/*",
    "README.md",
    "LICENSE",
    "CODE_OF_CONDUCT.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mu-ts/rest_endpoints.git"
  },
  "keywords": [
    "AWS",
    "httpEvent",
    "API Gateway",
    "Lambda",
    "endpoints",
    "http"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jeremyosborne/packagejsonexample/blob/master/LICENSE.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/mu-ts/rest_endpoints/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "chai": "4.4.1",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.3",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tslint": "^5.20.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@mu-ts/logger": "^3.2.3",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "ajv-keywords": "^5.1.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
